{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true
    },
    "plugins": ["jest"],
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/no-dynamic-require": 0,
        "global-require": 0
    }
}
